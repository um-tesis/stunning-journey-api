    "apollo-server-micro": "^3.11.1",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.4",
    "graphql": "^15.8.0",
    "next": "13.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "typescript": "^4.9.4"
  }
